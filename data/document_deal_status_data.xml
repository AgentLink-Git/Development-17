        <field name="code">unconditional_sale</field>
        <field name="name">Unconditional Sale</field>
    </record>
    <record id="status_removal_of_conditions" model="document.deal.status">
        <field name="code">removal_of_conditions</field>
        <field name="name">Removal of Conditions</field>
    </record>
    <record id="status_pending" model="document.deal.status">
